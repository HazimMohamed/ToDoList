{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "./bin",
    "esModuleInterop": true
  },
  "include": ["src/*",],
  "exclude": ["node_modules", "**/*.spec.ts"]
}